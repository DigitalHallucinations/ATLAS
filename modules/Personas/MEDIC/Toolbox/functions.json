[
    {
        "name": "google_search",
        "version": "1.0.0",
        "side_effects": "none",
        "default_timeout": 30,
        "auth": {
            "required": true,
            "type": "api_key",
            "env": "GOOGLE_API_KEY",
            "docs": "Set GOOGLE_API_KEY and GOOGLE_CSE_ID to use Google Programmable Search. When those are missing, configure SERPAPI_KEY as a fallback."
        },
        "allow_parallel": true,
        "idempotency_key": false,
        "description": "A Google search result API. When you need a short and clear answer to a specific question, you can use it. The input should be a search query.",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "description": "The search content."
                },
                "k": {
                    "type": "integer",
                    "default": 10,
                    "description": "Select the first k results in the search results as the response.",
                    "minimum": 1
                }
            },
            "required": [
                "query"
            ]
        },
        "cost_per_call": 0.005,
        "cost_unit": "USD",
        "capabilities": [
            "web_search",
            "knowledge_lookup"
        ],
        "providers": [
            {
                "name": "google_cse",
                "priority": 0,
                "health_check_interval": 300,
                "config": {
                    "api_key_env": "GOOGLE_API_KEY",
                    "api_key_config": "GOOGLE_API_KEY",
                    "cse_id_env": "GOOGLE_CSE_ID",
                    "cse_id_config": "GOOGLE_CSE_ID"
                }
            },
            {
                "name": "serpapi",
                "priority": 10,
                "health_check_interval": 300
            }
        ]
    },
    {
        "name": "search_pubmed",
        "version": "1.0.0",
        "side_effects": "none",
        "default_timeout": 30,
        "auth": {
            "required": false,
            "env": "NCBI_API_KEY",
            "docs": "Optional Entrez API key for higher rate limits."
        },
        "allow_parallel": true,
        "idempotency_key": false,
        "capabilities": [
            "medical_research",
            "literature_search"
        ],
        "providers": [
            {
                "name": "ncbi_entrez",
                "priority": 0,
                "health_check_interval": 300
            }
        ],
        "description": "Search PubMed for biomedical literature using the NCBI Entrez API.",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "description": "The search query."
                },
                "max_results": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 500,
                    "default": 20,
                    "description": "The maximum number of PubMed IDs to return."
                },
                "page_size": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 100,
                    "description": "Batch size to request from the Entrez API."
                },
                "sort": {
                    "type": "string",
                    "description": "Optional Entrez sort order (e.g., 'pub+date')."
                },
                "datetype": {
                    "type": "string",
                    "description": "Date field to filter on (e.g., PDAT)."
                },
                "mindate": {
                    "type": "string",
                    "description": "Earliest date to include (YYYY/MM/DD)."
                },
                "maxdate": {
                    "type": "string",
                    "description": "Latest date to include (YYYY/MM/DD)."
                }
            },
            "required": [
                "query"
            ]
        }
    },
    {
        "name": "search_pmc",
        "version": "1.0.0",
        "side_effects": "none",
        "default_timeout": 30,
        "auth": {
            "required": false,
            "env": "NCBI_API_KEY",
            "docs": "Optional Entrez API key for higher rate limits."
        },
        "allow_parallel": true,
        "idempotency_key": false,
        "capabilities": [
            "medical_research",
            "literature_search"
        ],
        "providers": [
            {
                "name": "ncbi_entrez",
                "priority": 0,
                "health_check_interval": 300
            }
        ],
        "description": "Search PubMed Central (PMC) for open access articles using the Entrez API.",
        "parameters": {
            "type": "object",
            "properties": {
                "query": {
                    "type": "string",
                    "description": "The search query."
                },
                "max_results": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 500,
                    "default": 20,
                    "description": "The maximum number of PMC IDs to return."
                },
                "page_size": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 100,
                    "description": "Batch size to request from the Entrez API."
                },
                "article_type": {
                    "type": "string",
                    "description": "Optional PMC article type filter."
                },
                "has_abstract": {
                    "type": "boolean",
                    "description": "Limit results to articles with abstracts."
                }
            },
            "required": [
                "query"
            ]
        }
    },
    {
        "name": "get_current_info",
        "version": "1.0.0",
        "side_effects": "none",
        "default_timeout": 5,
        "auth": {
            "required": false
        },
        "allow_parallel": true,
        "idempotency_key": false,
        "description": "Get the current time, date, day, month and year, or timestamp based on the format specified.",
        "parameters": {
            "type": "object",
            "properties": {
                "format_type": {
                    "type": "string",
                    "enum": [
                        "time",
                        "date",
                        "day",
                        "month_year",
                        "timestamp"
                    ],
                    "default": "timestamp"
                }
            },
            "required": []
        },
        "cost_per_call": 0.0,
        "cost_unit": "USD",
        "capabilities": [
            "time_information",
            "date_information"
        ]
    }
]
